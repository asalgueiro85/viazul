{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Bauche</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ entity.codigo }}</td>
            </tr>
            <tr>
                <th>Importe</th>
                <td>{{ entity.importe }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Origen</th>
                <td>{{ entity.origen }}</td>
            </tr>
            <tr>
                <th>Destino</th>
                <td>{{ entity.destino }}</td>
            </tr>
            <tr>
                <th>Area</th>
                <td>{{ entity.area }}</td>
            </tr>
            <tr>
                <th>Dirarea</th>
                <td>{{ entity.dirArea }}</td>
            </tr>
            <tr>
                <th>Fechaemitido</th>
                <td>{{ entity.fechaEmitido|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechaconsumo</th>
                <td>{{ entity.fechaConsumo|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('bauche') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('bauche_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
