{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Configuracion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Cuenta</th>
                <td>{{ entity.cuenta }}</td>
            </tr>
            <tr>
                <th>Fondo</th>
                <td>{{ entity.fondo }}</td>
            </tr>
            <tr>
                <th>Trans</th>
                <td>{{ entity.trans }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('configuracion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('configuracion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
